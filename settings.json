{
  "listen": {
    "host": "0.0.0.0",
    "port": 3002
  },
  "requestTimeoutDefault": 60,
  "sseReadTimeoutDefault": 0,
  "sseMaxBodyBytes": 0,
  "sseMaxDurationSec": 0,
  "onLimit": "504",
  "tls": {
    "rejectUnauthorized": true,
    "caFile": ""
  },
  "cors": {
    "enabled": false,
    "allowedOrigins": []
  },
  "sse": {
    "responseContentType": "text/plain; charset=utf-8",
    "aggregationMode": "raw"
  },
  "upstream": {
    "allowedHosts": ["localhost", "test-server", "host.docker.internal"]
  },
  "limits": {
    "maxRequestBodyBytes": 0
  },
  "health": {
    "enabled": true,
    "paths": {
      "healthz": "/healthz",
      "ready": "/ready"
    }
  },
  "test": {
    "serverHost": "test-server",
    "serverPort": 8081
  }
}


